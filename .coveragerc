[run]
source = .
omit =
    */tests/*
    */migrations/*
    */__pycache__/*
    */site-packages/*
    *test*.py
    final_test.py
    simple_test.py
    specific_line_coverage.py
    views_coverage.py
    manage.py
    config/wsgi.py
    config/asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if settings.DEBUG
    if 0:
    if TYPE_CHECKING:
    class .*Factory
    @staticmethod
    @classmethod
